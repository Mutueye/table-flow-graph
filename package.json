{
  "name": "table-flow-graph",
  "version": "0.2.6",
  "description": "A simple flow graph editor/viewer",
  "author": "mutueye <mutuyueniao@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mutueye/table-flow-graph"
  },
  "main": "dist/index.common.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "table",
    "flow",
    "graph"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "watch": "rimraf dist && rollup -cw",
    "lint": "eslint --ext .js,.ts --ignore-path .eslintignore --fix src",
    "prepare": "simple-git-hooks",
    "docs:dev": "pnpm -C docSrc dev"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^8.0.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.1",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.49.9",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.6.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.1",
    "lodash-es": "^4.17.21"
  }
}
